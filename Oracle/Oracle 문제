#문제 1번
SELECT DEPTNO , 
ROUND(AVG(SAL),0)AS AVG_SAL , 
MAX(SAL)AS MAX_SAL ,
MIN(SAL)AS MIN_SAL ,
COUNT(ENAME)AS CNT 
FROM EMP
GROUP BY DEPTNO;

#문제 2번
SELECT JOB,COUNT(*) FROM EMP
GROUP BY JOB
HAVING COUNT(*) >=3;

#문제 3번
SELECT DEPTNO, TO_CHAR(HIREDATE, 'YYYY') AS HIRE_YEAR,
DEPTNO, COUNT(ENAME) AS CNT
FROM EMP
GROUP BY TO_CHAR(HIREDATE,'YYYY'),DEPTNO;


#문제 4번
SELECT NVL2(COMM, 'O', 'X') AS EXIST_COMM, COUNT(*) AS CNT FROM EMP
GROUP BY NVL2(COMM, 'O', 'X');
