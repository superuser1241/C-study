using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Q5_3의배수
{
    internal class Program
    {
        static void Main(string[] args)
        {
            //1~100사이의 값 중 짝수만 출력
            for (int i = 1; i <= 100; i++)
                if(i %2==0)
                {
                    Console.WriteLine(i);
                }
            {
            }
        }
    }
}
