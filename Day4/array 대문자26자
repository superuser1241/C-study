using System;

namespace Array연속된_메모리
{
    internal class Program
    {
        static void Main(string[] args)
        {
            //연속된 메모리 공간에 알파벳 대문자 26자를 넣어보시오
            char[] arr = new char[26];

            char ch = 'A';
            for (int i = 0; i < arr.Length; i++)
            {
                arr[i] = (char)ch++;
            }
            for(int j=0; j< arr.Length; j++)
            {
                Console.WriteLine(arr[j]);
            }
        }
    }
}
