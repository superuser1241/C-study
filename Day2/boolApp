using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace boolApp
{
    internal class Program
    {
        static void Main(string[] args)
        {
            bool a = true;                      // bool은 int 불가능
            bool b = false;

            Console.WriteLine(a);
            Console.WriteLine(b);

            object c = 123;
            Console.WriteLine(c);

            object d = 3.141592;
            Console.WriteLine(d);

            object e = "안녕하세요";
            Console.WriteLine(e);

        }
    }
}
